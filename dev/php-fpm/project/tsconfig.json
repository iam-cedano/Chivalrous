{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./resources/js/*"],
      "@/types/client/*": ["./resources/js/pages/client/routes/home/types/*"],
      "@/data/client/*": ["resources/js/pages/client/routes/home/data/*"]
    },
    "typeRoots": ["resources/js/modules"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}