{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./resources/js/*"],
      "@/assets/*": ["./public/assets/*"]
    },
    "typeRoots": ["./resources/js/types/"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}